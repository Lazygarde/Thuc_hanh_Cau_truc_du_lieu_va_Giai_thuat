#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
vector<int> a(1000001, 1);
void era()
{
    for (int i = 2; i <= 1000000; i++)
    {
        for (int j = i * 2; j <= 1000000; j += i)
            a[j] += i;
    }
}
int main()
{
    ios_base::sync_with_stdio(0);
    cin.tie(0);
    era();
    int l, r, s = 0;
    cin >> l >> r;
    for (int i = l; i <= r; i++)
        if (a[i] > i)
            s++;
    cout << s;
}
