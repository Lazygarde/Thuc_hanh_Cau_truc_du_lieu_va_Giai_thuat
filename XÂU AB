#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
    ios_base::sync_with_stdio(0);
    cin.tie(0);
    int t;
    cin >> t;
    while (t--)
    {
        int n, d = 0;
        cin >> n;
        string a;
        for (int i = 0; i < n; i++)
            a += 'A';
        cout << a;
        while (1)
        {
            if (d != 0)
                cout << "," << a;
            d++;
            int ok = 0;
            for (int i = n - 1; i >= 0; i--)
            {
                if (a[i] == 'A')
                {
                    ok = 1;
                    a[i] = 'B';
                    for (int j = i + 1; j < n; j++)
                        a[j] = 'A';
                    break;
                }
            }
            if (ok == 0)
                break;
        }
        cout << endl;
    }
}
