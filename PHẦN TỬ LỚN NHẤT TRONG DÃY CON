#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int main()
{
    ios_base::sync_with_stdio(0);
    cin.tie(0);
    int t;
    cin >> t;
    while (t--)
    {
        int n, k;
        cin >> n >> k;
        int a[n];
        deque<int> d;
        for (int i = 0; i < n; i++)
        {
            cin >> a[i];
            while (d.size() && a[d.back()] <= a[i])
                d.pop_back();
            d.push_back(i);
            while (d.size() && d.front() <= i - k)
                d.pop_front();
            if (i >= k - 1)
                cout << a[d.front()] << " ";
        }
        cout << endl;
    }
}
