#include <bits/stdc++.h>
#define endl "\n"
using namespace std;
int n;
vector<int> a;
vector<vector<int>> b;
void Try(int k, int pos)
{
    if (k == n)
        b.push_back(a);
    for (int i = pos; i >= 1; i--)
    {
        if (i + k <= n)
        {
            a.push_back(i);
            Try(i + k, i);
            a.pop_back();
        }
    }
}
int main()
{
    ios_base::sync_with_stdio(0);
    cin.tie(0);
    int t;
    cin >> t;
    while (t--)
    {
        b.clear();
        cin >> n;
        for (int i = n; i >= 1; i--)
        {
            a.push_back(i);
            Try(i, i);
            a.pop_back();
        }
        cout << b.size() << endl;
        for (int i = 0; i < b.size(); i++)
        {
            cout << "(" << b[i][0];
            for (int j = 1; j < b[i].size(); j++)
                cout << " " << b[i][j];
            cout << ") ";
        }
        cout << endl;
    }
}
